{
    "manifest_version": 3,
    "name": "Google Translate OCR",
    "description": "OCR in Google Translate",
    "version": "1.0",
    "icons": {
        "16": "icons/icon-16.png",
        "32": "icons/icon-32.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "content_scripts": [
        {
            "js" : ["ocr.js"],
            "css" : ["ocr.css"],
            "matches": ["*://*.translate.google.com/*"],
            "run_at": "document_end"
        }
    ]
}